{
  "name": "pdi-component-library",
  "version": "0.0.1",
  "description": "Painted Dogs Interactive Component Library",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1"
  },
  "devDependencies": {
    "@stencil/core": "^0.11.0",
    "@stencil/sass": "^0.1.0",
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kriswinbush/pdi-component-library.git"
  },
  "author": "Kris J Winbush",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kriswinbush"
  },
  "homepage": "https://github.com/kriswinbush",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
